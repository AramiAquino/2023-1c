@{
}
@model List<SeleccionViewModel>
<h1>Selecciones sudamericanas eliminatorias Mundial 2026</h1>
<div>
    <ul>
        @foreach (var seleccion in Model.OrderBy(s=> s.Nombre).ToList())
        {
            <li>@seleccion.Nombre (@seleccion.Apodo)</li>
        }
    </ul>
</div>